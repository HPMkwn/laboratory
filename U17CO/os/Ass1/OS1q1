echo "Enter Char : "
read ch
case $ch in
        a|e|i|o|u)
                echo "Vovel"
                ;;
        [a-z])
                echo "Lower case"
                ;;
        [A-Z])
                echo "Upper Case"
                ;;
        [0-9])
                echo "Digit"
                ;;
        *)
                echo "Special Char"
                ;;
        esac
